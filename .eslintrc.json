{
    "env": {
        "browser": true,
        "es6": true
    },
	"globals": {
		"ENV": true
	},
    "extends": "standard",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "quotes": [
            "error",
            "single"
        ]
    }
}
