{
  "scripts": {
    "copyfiles": "mkdir -p dist; cp index.html dist/index.html;  cp -a css dist/",
    "build": "npm run copyfiles && rollup -c",
    "build:min": "npm run copyfiles && rollup -c --environment production",
    "dev": "npm run copyfiles && rollup -c -w",
    "start": "http-server dist & npm run dev",
    "test": "eslint src/ && ava"
  },
  "files": [
    "index.html",
    "dist"
  ],
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "6.18.0",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-es2015-rollup": "1.2.0",
    "babel-register": "6.23.0",
    "babel-runtime": "6.23.0",
    "eslint": "4.1.0",
    "eslint-plugin-html": "3.0.0",
    "http-server": "0.9.0",
    "jsdom": "9.11.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-svelte": "^1.0.0",
    "rollup-watch": "^3",
    "standard": "^8.6.0",
    "svelte": "1.23.4",
    "svelte-cli": "1.3.6"
  },
  "dependencies": {
    "lodash-es": "^4.17.4"
  },
  "name": "waves.fet.li",
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": "git@github.com:Hexagon6/wave.git",
  "author": "Tobi Turing <dev@fet.li>",
  "license": "MIT",
  "ava": {
    "files": [
      "src/tests/*.js"
    ],
    "source": [
      "src"
    ],
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }

        ]
      ],
      "plugins": [
        "external-helpers",
        "transform-runtime"
      ]
    }
  }
}
